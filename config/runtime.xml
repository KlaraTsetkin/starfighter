<?xml version="1.0" encoding="UTF-8"?>
<dmz>
<runtime>
   <time>
      <frequency value="0.0"/>
   </time>
   <state name="Engine_On"/>
   <state name="Dead"/>
   <state name="Smoking"/>
   <state name="Fire"/>
   <event-type name="Event_Launch">
      <audio resource="launch-sound"/>
      <net>
         <enum value="16"/>
      </net>
   </event-type>
   <event-type name="Event_Detonation">
      <audio resource="detonation-sound"/>
      <net>
         <enum value="17"/>
      </net>
      <render>
         <image resoure="explosion"/>
      </render>
   </event-type>
   <event-type name="Event_Collision">
      <net>
         <enum value="18"/>
      </net>
   </event-type>
   <object-type name="spectator">
   </object-type>
   <object-type name="munitions">
      <render>
         <isect disable="true"/>
      </render>
      <net>
         <enum value="2"/>
         <rule type="heartbeat" value="1.0"/>
         <rule type="zero-velocity"/>
         <rule type="rate-limit"/>
         <rule type="position" value="10.0"/>
         <rule type="orientation"/>
         <rule type="velocity"/>
      </net>
   </object-type>
   <object-type name="round" parent="munitions">
      <timeout value="5" detonate="true"/>
      <render>
         <isect disable="true"/>
         <model resource="round"/>
      </render>
      <net>
         <enum value="2.1"/>
      </net>
   </object-type>
   <object-type name="vehicle">
      <net>
         <enum value="1"/>
      </net>
   </object-type>
   <object-type name="space-vehicle" parent="vehicle">
      <net>
         <enum value="1.1"/>
         <state name="Engine_On" mask="0x001"/>
         <state name="Dead" mask="0x002"/>
         <state name="Smoking" mask="0x008"/>
         <state name="Fire" mask="0x010"/>
         <rule type="heartbeat" value="1.0"/>
         <rule type="zero-velocity"/>
         <rule type="rate-limit"/>
         <rule type="position" value="10.0"/>
         <rule type="orientation"/>
         <rule type="velocity"/>
         <rule type="state"/>
      </net>
   </object-type>
   <object-type name="viper-2" parent="space-vehicle">
      <render>
         <model resource="viper-2"/>
<!--
         <model resource="explosion" state="Dead"/>
-->
      </render>
      <net>
         <enum value="1.1.1"/>
      </net>
   </object-type>
   <object-type name="viper-7" parent="space-vehicle">
      <render>
         <model resource="viper-7"/>
      </render>
      <net>
         <enum value="1.1.2"/>
      </net>
   </object-type>
   <object-type name="colonial-bomber" parent="space-vehicle">
      <render>
         <model resource="colonial-bomber"/>
      </render>
      <net>
         <enum value="1.1.3"/>
      </net>
   </object-type>
   <object-type name="colonial-one" parent="space-vehicle">
      <render>
         <model resource="colonial-one"/>
      </render>
      <net>
         <enum value="1.1.4"/>
      </net>
   </object-type>
   <object-type name="pegasus" parent="space-vehicle">
      <render>
         <model resource="pegasus"/>
      </render>
      <net>
         <enum value="1.1.5"/>
      </net>
   </object-type>
   <object-type name="raider" parent="space-vehicle">
      <render>
         <model resource="raider"/>
      </render>
      <net>
         <enum value="1.1.6"/>
      </net>
   </object-type>
   <object-type name="heavy-raider" parent="space-vehicle">
      <render>
         <model resource="heavy-raider"/>
      </render>
      <net>
         <enum value="1.1.7"/>
      </net>
   </object-type>
   </runtime>
</dmz>

<?xml version="1.0" encoding="UTF-8"?>
<dmz>
<plugin-list>
   <plugin name="dmzWeaponPluginFixedLauncher" unique="right-raider-cannon"/>
   <plugin name="dmzWeaponPluginFixedLauncher" unique="left-raider-cannon"/>

</right-raider-cannon>
<left-raider-cannon>
   <button value="-1"/>
   <launch-message name="Raider_Launch_Message"/>
   <offset x="-1.16" y="-0.23" z="-5"/>
   <munitions>
      <object-type name="raider-round"/>
   </munitions>
   <delay value="0.5"/>
</left-raider-cannon>
</dmz>

